{
  "formItems": [
    {
      "type": "editor-form-style-text-input",
      "elFormItemProps": {
        "prop": "title",
        "label": "Title Text"
      },
      "elFormItemChildProps": {
        "placeholder": "Please enter the Text content",
        "defaultValue": "Here is the title title"
      }
    },
    {
      "type": "editor-form-style-text-input",
      "elFormItemProps": {
        "prop": "content",
        "label": "Content Text"
      },
      "elFormItemChildProps": {
        "placeholder": "Please enter the Text content",
        "defaultValue": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed fringilla, quam ut consequat suscipit, mauris felis venenatis lectus, at ultrices est leo vel justo. Praesent vitae erat non velit vehicula blandit. Maecenas sed condimentum dui. Vivamus malesuada, lacus eu aliquet dignissim, odio enim vestibulum ante."
      }
    },
    {
      "type": "img-uploader",
      "elFormItemProps": {
        "prop": "imageLarge",
        "label": "Large Image"
      },
      "elFormItemChildProps": {
        "placeholder": "place input the img src"
      }
    },
    {
      "type": "img-uploader",
      "elFormItemProps": {
        "prop": "imageSmall1",
        "label": "Small Image 1"
      },
      "elFormItemChildProps": {
        "placeholder": "place input the img src"
      }
    },
    {
      "type": "img-uploader",
      "elFormItemProps": {
        "prop": "imageSmall2",
        "label": "Small Image 2"
      },
      "elFormItemChildProps": {
        "placeholder": "place input the img src"
      }
    },
    {
      "type": "el-switch",
      "elFormItemProps": {
        "prop": "darkTheme",
        "label": "Theme"
      },
      "elFormItemChildProps": {
        "active-text": "Dark theme",
        "inactive-text": "Bright color theme",
        "@change": "handleSubmit"
      }
    }
  ],
  "i18n": {
    "en": {
      "Title Text": "Title Text",
      "Content Text": "Content Text",
      "Large Image": "Large Image",
      "Small Image 1": "Small Image 1",
      "Small Image 2": "Small Image 2",
      "Theme": "Theme"
    },
    "zh": {
      "Title Text": "标题文本",
      "Content Text": "内容文本",
      "Large Image": "大图",
      "Small Image 1": "小图 1",
      "Small Image 2": "小图 2",
      "Theme": "主题"
    }
  }
}
